        <a class="navbar-brand" href="#">Serverless Playstation</a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>

        <div class="collapse navbar-collapse" id="navbarSupportedContent">
            <ul class="navbar-nav mr-auto">
                <li class="nav-item active">
                    <a asp-page="/Index" class="nav-link">Home <span class="sr-only">(current)</span></a>
                    @*<a class="nav-link" href="#">Home <span class="sr-only">(current)</span></a>*@
                </li>
                @*<li class="nav-item">
                        <a asp-page="/Index" class="nav-link">Index</a>
                    </li>*@
                <li class="nav-item">
                    <a asp-page="/Movies" class="nav-link">Movies</a>
                </li>

                <li class="nav-item">
                    @*<a asp-page="/LeetCode" class="nav-link">LeetCode</a>*@
                    <a asp-controller="ImageRekognition" asp-action="Index" class="nav-link">LeetCode</a>
                </li>

                <li class="nav-item">
                    <a asp-page="/Help" class="nav-link">Help</a>
                </li>

                <li class="nav-item">
                    <a asp-page="/CreateEC2" class="nav-link">EC2</a>
                </li>

                <li class="nav-item">
